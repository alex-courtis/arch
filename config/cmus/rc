set resume=true

set mouse=true
set wrap_search=false

set aaa_mode=artist
set continue_album=false

set tree_width_percent=15

set format_statusline= %{status}  %{position} / %{duration}%=%{?volume>=0?%{volume}%% }

set format_trackwin_va=%3n. %{?c=""?%t?%-35%t %c} %= %-25%a %y %d
set    format_trackwin=%3n. %{?c=""?%t?%-35%t %c} %= %y %d 

bind -f common j win-down
bind -f common k win-up
bind -f common h win-next
bind -f common l win-next

bind -f common tab win-next

bind -f common enter win-toggle

bind -f common ^U win-half-page-up
bind -f common ^D win-half-page-down
bind -f common g win-top
bind -f common m win-page-middle
bind -f common G win-bottom
 
bind -f common ? view settings
bind -f settings ? view tree
 
bind -f common / search-start
bind -f common n search-next
bind -f common N search-prev
 
bind -f common backspace shell cmus-play >/dev/null 2>&1
bind -f common mlb_click_selected shell cmus-play >/dev/null 2>&1
bind -f common space shell cmus-pause
 
bind -f common down player-next
bind -f common up player-prev

bind -f common left seek -5
bind -f common right seek +5

bind -f common + vol +5%
bind -f common - vol -5%

bind -f common q quit

colorscheme default.amc
